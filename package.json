{
  "name": "obsidian-tag-navigator",
  "version": "0.5.1",
  "description": "Tag Navigator plugin for Obsidian: Cross-reference navigation through your tags",
  "author": "alexobenauer",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -c -w",
    "lint": "svelte-check && eslint . --ext .ts",
    "build": "npm run lint && rollup -c",
    "build:nolint": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test -- --watch"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "obsidian": "github:obsidianmd/obsidian-api#master",
    "svelte": "^5.46.4",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@tsconfig/svelte": "^5.0.6",
    "@types/jest": "29.2.3",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "dependency-cruiser": "^17.3.7",
    "eslint": "8.28.0",
    "jest": "29.3.1",
    "rollup": "^4.55.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.2.3",
    "svelte-check": "^4.3.5",
    "svelte-jester": "^5.0.0",
    "svelte-preprocess": "^6.0.3",
    "ts-jest": "29.0.3",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ]
  }
}
